{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Vue.js 2.x Tsconfig",

  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "preserveValueImports": true,
    "experimentalDecorators": true,
    "importsNotUsedAsValues": "error",
    "lib": ["ESNext", "DOM", "DOM.Iterable"]
  }
}
