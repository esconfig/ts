{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Base Tsconfig",

  "compilerOptions": {
    "strict": true,
    "lib": ["ESNext"],
    "target": "ESNext",
    "module": "ESNext",
    "importHelpers": true,
    "removeComments": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}
